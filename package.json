  "version": "0.1.0",
  "private": false,
  "engines": {
    "node": "0.10.x",
    "npm": "1.2.x"
  },
  "dependencies": {
    "restify": "^4.0.3",
    "config-multipaas": "0.2.2",
    "pg": "^4.4.3",
    "pg-query": "^0.11.0",
    "body-parser": "^1.8.3",
    "express": "^4.9.4",
    "jwt-simple": "^0.2.0",
    "morgan": "^1.3.1"
  },
  "devDependencies": {
    "tap": "^5.7.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dollys21/restify-postGIS.git"
  },
  "scripts": {
